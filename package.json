{
	"name": "logiscore-frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"description": "LogiScore - Freight Forwarder Review Platform",
	"keywords": ["svelte", "sveltekit", "vercel", "logistics", "reviews"],
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"vercel-build": "vite build"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@sveltejs/adapter-vercel": "^5.8.2"
	}
}
